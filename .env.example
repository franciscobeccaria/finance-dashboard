# Environment variables for local development
# Copy this file to .env.local and fill in your actual values

# Backend URL
NEXT_PUBLIC_BACKEND_URL=http://localhost:3001

# Auth.js configuration
AUTH_SECRET=your_auth_secret_here_64_characters_long
NEXTAUTH_URL=http://localhost:3000

# Google OAuth credentials
# Get these from Google Cloud Console - OAuth 2.0 Client IDs
GOOGLE_CLIENT_ID=your_google_client_id.apps.googleusercontent.com
GOOGLE_CLIENT_SECRET=your_google_client_secret

# Production settings (for Vercel deployment)
# NEXTAUTH_URL=https://your-app-domain.vercel.app
# NEXT_PUBLIC_BACKEND_URL=https://your-backend-domain.herokuapp.com